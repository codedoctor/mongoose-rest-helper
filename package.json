{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "mongoose-rest-helper",
  "description": "A bunch of helpers used to simplify rest development.",
  "version": "0.1.7",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/mongoose-rest-helper.git"
  },
  "bin": {},
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "keywords": [
    "mongoose"
  ],
  "scripts": {
    "prepublish": "node_modules/.bin/codo -i ./src -o ./docs;node_modules/.bin/grunt coffee",
    "test": "node_modules/.bin/grunt testandcoverage",
    "docs": "node_modules/.bin/codo -i ./src -o ./docs"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "underscore": "1.5.2",
    "mongoose": ">=3.6.2",
    "mongodb-objectid-helper": "0.2.0",
    "underscore-ext": "0.2.0",
    "date-utils": "1.2.14",
    "some-errors": "0.2.0",
    "async": "0.2.9",
    "simple-paginator": "0.2.1"
  },
  "devDependencies": {
    "blanket": "1.1.6",
    "codo": "2.0.9",
    "coffee-script": "1.7.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-coffee": "0.10.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-env": "0.4.1",
    "grunt-mocha-cov": "0.2.1",
    "grunt-mocha-test": "0.11.0",
    "grunt-release": "0.7.0",
    "grunt-shell": "0.7.0",
    "matchdep": "0.3.0",
    "mocha": "1.13.0",
    "mongoskin": "0.6.0",
    "should": "2.0.1"
  },
  "bugs": {
    "url": "http://github.com/codedoctor/mongoose-rest-helper/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/codedoctor/mongoose-rest-helper/raw/master/LICENSE"
    }
  ],
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
